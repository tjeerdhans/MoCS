@model List<string>
@{
    var selectListItems = ViewData["selectListItems"] as IEnumerable<SelectListItem>;
}
    @Html.ListBox("", selectListItems, new { @class = "select2" })

<script>
    $(function () {
        $("option[value=Disabled]").attr("disabled", "disabled");
        $(".select2").select2({ containerCss: { 'width': '100%' } });
    });
</script>
